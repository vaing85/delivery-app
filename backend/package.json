{
  "name": "delivery-app-backend",
  "version": "1.0.0",
  "description": "Backend API server for delivery management application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:migrate:postgres": "prisma migrate dev --schema=prisma/schema.postgresql.prisma",
    "db:push:postgres": "prisma db push --schema=prisma/schema.postgresql.prisma",
    "db:generate:postgres": "prisma generate --schema=prisma/schema.postgresql.prisma",
    "db:migrate:data": "node scripts/migrate-to-postgresql.js"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@types/crypto-js": "^4.2.2",
    "@types/socket.io": "^3.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
