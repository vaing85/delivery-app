    "@types/nodemailer": "^6.4.12",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^6.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
